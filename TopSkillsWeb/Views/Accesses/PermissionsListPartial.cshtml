@model List<Core.Accesses.UserPermissions>

@if (Model.Count() > 0)
{
    @foreach (var item in Model)
    {
        <a onclick="onShowPermissionDetail('@item.PermissionId', event)" class="list-group-item list-group-item-action permission_item" style="cursor: pointer;">@item.Description</a>
    }
}
else
{
    <a href="#" class="list-group-item list-group-item-action permission_item">@Resource.EmptyEntry</a>
}